#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n, max=0;
    cin >> n;
    int height[n];
    for (int i = 0; i < n; ++i) {
        cin >> height[i];        
    }
    max=height[0];
    for (int j = 1; j < n; ++j) {
        if (height[j]>=max) {
            max=height[j];}
    }
    cout << max;
    return 0;
}
